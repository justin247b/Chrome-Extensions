{
    "manifest_version": 3,
    "name": "Highlight extension",
    "version": "1.0",
    "content_scripts": [
      {
        "js": ["script.js"],
        "matches": ["<all_urls>"],
        "run_at": "document_end"
      }
    ],
    "action":{
      "default_popup": "index.html",
      "default_title": "Highlight Text"
  },
    "web_accessible_resources": [{
        "resources": ["index.html"],
        "matches": [],
        "extension_ids": []
}]
    
}
